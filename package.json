{
  "name": "quiz-funnel-backend",
  "version": "1.0.0",
  "description": "Quiz Funnel Backend API",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "seed": "ts-node src/scripts/seed.ts",
    "migrate": "ts-node src/scripts/migrate.ts",
    "migration-status": "ts-node src/scripts/migration-status.ts",
    "check-schema": "ts-node src/scripts/check-schema.ts",
    "init-db": "ts-node src/scripts/init-db.ts",
    "set-admin-password": "ts-node src/scripts/set-admin-password.ts"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "express-validator": "^7.3.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "resend": "^6.5.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.8",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "quiz",
    "funnel",
    "api",
    "backend"
  ],
  "author": "Quiz Funnel Team",
  "license": "MIT"
}
